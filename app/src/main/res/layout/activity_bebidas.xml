<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".BebidasActivity">

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="413dp"
        android:layout_height="92dp"
        android:background="#D9D9D9"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="230dp"
            android:layout_height="60dp"
            android:layout_margin="23dp"
            android:layout_weight="1"
            android:fontFamily="@font/jomhuria"
            android:text="Bebidas"
            android:textAlignment="viewStart"
            android:textColor="#000000"
            android:textSize="40sp" />

        <ImageButton
            android:id="@+id/carritoButton"
            android:layout_width="5dp"
            android:layout_height="40dp"
            android:layout_marginHorizontal="30dp"
            android:layout_marginTop="30dp"
            android:layout_weight="1"
            android:background="@drawable/carrito" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout9"
        android:layout_width="224dp"
        android:layout_height="72dp"
        android:layout_marginTop="12dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.219"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout8">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/jomhuria"
            android:text="Conoce nuestro menú"
            android:textColor="#000000"
            android:textSize="40sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout13"
        android:layout_width="327dp"
        android:layout_height="489dp"
        android:layout_marginBottom="50dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout9"
        app:layout_constraintVertical_bias="1.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="82dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/ic_field"

            android:backgroundTint="#E8E8E8"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="95dp"
                android:layout_height="match_parent"
                android:layout_marginVertical="10dp"
                android:layout_marginLeft="25dp"
                android:layout_weight="1"
                android:background="@drawable/embotellado" />

            <Button
                android:id="@+id/ButtonEmbotellado"
                android:layout_width="211dp"
                android:layout_height="match_parent"
                android:backgroundTint="#E8E8E8"
                android:fontFamily="@font/jomhuria"
                android:text="Embotellado"
                android:textAlignment="center"
                android:textColor="#CC000000"
                android:textSize="35sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="82dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/ic_field"
            android:backgroundTint="#E8E8E8"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/imageViewTacos"
                android:layout_width="95dp"
                android:layout_height="match_parent"
                android:layout_marginVertical="10dp"
                android:layout_marginLeft="25dp"
                android:layout_weight="1"
                android:background="@drawable/jugos" />

            <Button
                android:id="@+id/buttonJugos"
                android:layout_width="211dp"
                android:layout_height="match_parent"
                android:backgroundTint="#E8E8E8"
                android:fontFamily="@font/jomhuria"
                android:text="Jugos"
                android:textAlignment="center"
                android:textColor="#CC000000"
                android:textSize="35sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="82dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/ic_field"
            android:backgroundTint="#E8E8E8"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/imageViewEmpanadas"
                android:layout_width="95dp"
                android:layout_height="match_parent"
                android:layout_marginVertical="10dp"
                android:layout_marginLeft="25dp"
                android:layout_weight="1"
                android:background="@drawable/frape" />

            <Button
                android:id="@+id/buttonFrapes"
                android:layout_width="211dp"
                android:layout_height="match_parent"
                android:backgroundTint="#E8E8E8"
                android:fontFamily="@font/jomhuria"
                android:text="Frapes"
                android:textAlignment="center"
                android:textColor="#CC000000"
                android:textSize="35sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="82dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/ic_field"

            android:backgroundTint="#E8E8E8"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/imageViewCaldos"
                android:layout_width="95dp"
                android:layout_height="match_parent"
                android:layout_marginVertical="10dp"
                android:layout_marginLeft="25dp"
                android:layout_weight="1"
                android:background="@drawable/cafe" />

            <Button
                android:id="@+id/buttonCafe"
                android:layout_width="211dp"
                android:layout_height="match_parent"
                android:backgroundTint="#E8E8E8"
                android:fontFamily="@font/jomhuria"
                android:text="Café"
                android:textAlignment="center"
                android:textColor="#CC000000"
                android:textSize="35sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="82dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/ic_field"
            android:backgroundTint="#E8E8E8"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/imageViewCoctel"
                android:layout_width="95dp"
                android:layout_height="match_parent"
                android:layout_marginVertical="10dp"
                android:layout_marginLeft="25dp"
                android:layout_weight="1"
                android:background="@drawable/jugoembotellado" />

            <Button
                android:id="@+id/buttonJugoEmbotellado"
                android:layout_width="211dp"
                android:layout_height="match_parent"
                android:backgroundTint="#E8E8E8"
                android:fontFamily="@font/jomhuria"
                android:text="Jugo Embotellado"
                android:textAlignment="center"
                android:textColor="#CC000000"
                android:textSize="35sp" />
        </LinearLayout>


    </LinearLayout>
    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="415dp"
        android:layout_height="91dp"
        android:background="#32B4C0C0"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="73dp"
            android:background="#0CDACDCD"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/ImagenButtonCom"
                android:layout_width="10dp"
                android:layout_height="55dp"
                android:layout_marginHorizontal="20dp"
                android:layout_marginVertical="10dp"
                android:layout_weight="1"
                android:background="@drawable/plato"
                android:backgroundTint="#887676" />

            <ImageButton
                android:id="@+id/imageButtonBeb"
                android:layout_width="10dp"
                android:layout_height="55dp"
                android:layout_marginVertical="10dp"
                android:layout_weight="1"
                android:background="@drawable/jugo"
                android:backgroundTint="#FFC107" />

            <ImageButton
                android:id="@+id/imageButtonMen"
                android:layout_height="43dp"
                android:layout_margin="30dp"
                android:layout_weight="1"
                android:background="@drawable/menu0"
                android:backgroundTint="#8E7777"  android:layout_width="1dp"/>

            <ImageButton
                android:id="@+id/imageButtonMes"
                android:layout_width="10dp"
                android:layout_height="55dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                android:layout_weight="1"
                android:background="@drawable/mesa"
                android:backgroundTint="#8E7777" />

            <ImageButton
                android:id="@+id/imageButtonSal"
                android:layout_width="10dp"
                android:layout_height="55dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                android:layout_weight="1"
                android:background="@drawable/menu"
                android:backgroundTint="#8E7777" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView16"
                android:layout_width="32dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:fontFamily="@font/inter"
                android:text="Comidas"
                android:textAlignment="center"
                android:textColor="#100F0F"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="4dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="14dp"
                android:layout_weight="1"
                android:fontFamily="@font/inter"
                android:text="Bebidas"
                android:textAlignment="center"
                android:textColor="#FFC107"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="16dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_weight="1"
                android:fontFamily="@font/inter"
                android:text="Mesas"
                android:textAlignment="center"
                android:textColor="#191212"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/inter"
                android:text="Salon"
                android:textAlignment="center"
                android:textColor="#050404"
                android:textSize="12sp" />
        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>