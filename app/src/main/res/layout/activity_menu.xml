<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MenuActivity">

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="413dp"
        android:layout_height="92dp"
        android:background="#D9D9D9"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="230dp"
            android:layout_height="60dp"
            android:layout_margin="23dp"
            android:layout_weight="1"
            android:fontFamily="@font/jomhuria"
            android:text="Menú"
            android:textAlignment="viewStart"
            android:textColor="#000000"
            android:textSize="40sp" />

        <ImageButton
            android:id="@+id/carritoButton"
            android:layout_width="5dp"
            android:layout_height="40dp"
            android:layout_marginHorizontal="30dp"
            android:layout_marginTop="30dp"
            android:layout_weight="1"
            android:background="@drawable/carrito" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout9"
        android:layout_width="232dp"
        android:layout_height="114dp"
        android:layout_marginTop="8dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.301"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout8">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="220dp"
            android:layout_height="98dp"
            android:fontFamily="@font/jomhuria"
            android:text="¡Disfruta de los diferentes servicios!"
            android:textColor="#000000"
            android:textSize="40sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="190dp"
        android:layout_weight="1"
        android:foregroundGravity="center"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout10">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButtonMesa"
                android:layout_width="155dp"
                android:layout_height="138dp"
                android:layout_marginStart="45dp"
                android:background="@drawable/silla1" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="244dp"
                android:layout_height="match_parent"
                android:fontFamily="@font/jomhuria"
                android:text="Reservar Mesa"
                android:textAlignment="center"
                android:textColor="#CC000000"
                android:textSize="40sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButtonSal"
                android:layout_width="133dp"
                android:layout_height="132dp"
                android:layout_marginLeft="25dp"
                android:layout_weight="1"
                android:background="@drawable/reserva" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="192dp"
                android:layout_height="47dp"
                android:layout_weight="1"
                android:fontFamily="@font/jomhuria"
                android:text="Reservar Salón"
                android:textAlignment="center"
                android:textColor="#CC000000"
                android:textSize="40sp" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout10"
        android:layout_width="414dp"
        android:layout_height="174dp"
        android:layout_marginTop="8dp"
        android:foregroundGravity="center"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout9">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButtonComida"
                android:layout_width="177dp"
                android:layout_height="124dp"
                android:layout_marginStart="50dp"
                android:background="@drawable/tacos1" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="270dp"
                android:layout_height="46dp"
                android:fontFamily="@font/jomhuria"
                android:text="Comida"
                android:textAlignment="center"
                android:textColor="#CC000000"
                android:textSize="40sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButtonBebida"
                android:layout_width="150dp"
                android:layout_height="124dp"
                android:layout_marginLeft="30dp"
                android:layout_weight="1"
                android:background="@drawable/bebida" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:layout_weight="1"
                android:fontFamily="@font/jomhuria"
                android:text="Bebida"
                android:textAlignment="center"
                android:textColor="#CC000000"
                android:textSize="40sp" />

        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="415dp"
        android:layout_height="91dp"
        android:background="#32B4C0C0"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:layout_width="251dp"
            android:layout_height="73dp"
            android:layout_marginHorizontal="80dp"
            android:background="#0CDACDCD"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/ImagenButtonCom"
                android:layout_width="8dp"
                android:layout_height="55dp"
                android:layout_marginHorizontal="20dp"
                android:layout_marginVertical="10dp"
                android:layout_weight="1"
                android:background="@drawable/menu0"
                android:backgroundTint="#FFC107" />

            <ImageButton
                android:id="@+id/imageButtonMen"
                android:layout_width="1dp"
                android:layout_height="43dp"
                android:layout_margin="30dp"
                android:layout_weight="1"
                android:background="#32FCFFFF" />

            <ImageButton
                android:id="@+id/imageButtonPerfil"
                android:layout_width="10dp"
                android:layout_height="55dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                android:layout_weight="1"
                android:background="@drawable/perfil"
                android:backgroundTint="#B69999" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="329dp"
            android:layout_height="match_parent"
            android:layout_marginStart="10dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView16"
                android:layout_width="156dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:fontFamily="@font/inter"
                android:text="Menú"
                android:textAlignment="center"
                android:textColor="#FFC107"
                android:textSize="12sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/inter"
                android:text="Perfil"
                android:textAlignment="center"
                android:textColor="#050404"
                android:textSize="12sp" />
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>